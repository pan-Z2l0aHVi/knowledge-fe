{
	"extends": "../../tsconfig.base.json",
	"compilerOptions": {
		"baseUrl": ".",
		"jsx": "react-jsx",
		"target": "ESNext",
		"module": "ESNext",
		"outDir": "./types",
		"emitDeclarationOnly": true,
		"paths": {}
	},
	"include": [
		"./src/**/*"
	],
	"exclude": [
		"./dist/**/*",
		"./types/**/*",
		"**/*.test.ts",
		"**/*.spec.ts",
		"**/*.test.tsx",
		"**/*.spec.tsx"
	]
}