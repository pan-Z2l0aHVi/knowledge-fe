{
    "extends": [
        "eslint:recommended",
        "plugin:vue/vue3-essential",
        "../../.eslintrc.base.json"
    ],
    "parser": "vue-eslint-parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "parser": "@typescript-eslint/parser"
    },
    "plugins": [
        "vue",
        "@typescript-eslint"
    ],
    "rules": {
        "vue/multi-word-component-names": 0
    }
}
